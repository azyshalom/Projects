      </Dialog>
      <TextStyle Id="Tahoma8" FaceName="Tahoma" Size="8" Red="255" Green="255" Blue="255" />
      <TextStyle Id="Tahoma10" FaceName="Tahoma" Size="10" Red="0" Green="0" Blue="0" />
      <TextStyle Id="Tahoma12" FaceName="Tahoma" Size="12" Red="255" Green="0" Blue="0" />
      <TextStyle Id="Tahoma14" FaceName="Tahoma" Size="14" Red="0" Green="255" Blue="0" />
      <TextStyle Id="Tahoma16" FaceName="Tahoma" Size="16" Red="0" Green="0" Blue="255" />
    </UI>
  </Fragment>
