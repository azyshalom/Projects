<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2003/01/wi">
  <Fragment>
    <UI>
      <Dialog Id="TestDialog" Width="1" Height="1" TrackDiskSpace="yes">
        <Control Id="TestControl" Height="1" Width="1" Type="Text" Y="1" X="1">
          <Text SourceFile="$(env.WIX)\examples\data\test.txt" />
        </Control>
        <Control Id="TestControl2" Height="2" Width="2" Type="Text" Y="2" X="2" Text="[foo]Text[bar]" />
